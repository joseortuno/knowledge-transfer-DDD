{
  "name": "knowllege-transfer-ddd",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "json-server --watch src/db/db.json --port 4000",
    "reset_db": "cp ./src/original-db.json ./src/db/db.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "npx eslint ./src/** --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "axios": "0.26.0",
    "json-server": "0.17.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@react-native-community/eslint-config": "2.0.0",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "@vitejs/plugin-react": "1.0.7",
    "eslint": "7.14.0",
    "eslint-plugin-react": "7.28.0",
    "typescript": "4.5.5",
    "vite": "2.8.0"
  },
  "ignorePatterns": [
    "*/db/*.json"
  ]
}
